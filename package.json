{
  "name": "snake",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@types/node": "12.6.2",
    "@types/ramda": "^0.27.40",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "easy-peasy": "^5.0.3",
    "ramda": "^0.27.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rxjs": "^7.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "lint": "tsc --noEmit && eslint \"**/*.{tsx,ts}\"",
    "lint:fix": "yarn run lint --fix"
  },
  "eslintConfig": {
    "extends": "react-app",
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "eslint-plugin-prettier": "^3.4.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "homepage": "https://mateuszjonak.github.io/snake/"
}
